public class Fibonacci {
	public static void main(String args[]) {
		int n1=0,n2=1,n3,i;
		int[10000] Fibo;
		float[10000] Bolumler;
		Fibo[1] = 1;
		for(i=2;i<10000;++i)     //Döngü 1 den başlıyor çünkü ilk terim 0
		{
			n3=n1+n2;
			Fibo[i] = n3;
			Bolumler[i] = Fibo[i] / Fibo[i-1];
			n1=n2;
			n2=n3;
		}
		System.out.println();
	}
}
